{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React Redux Template",
  "main": "build/server/index.js",
  "scripts": {
    "eslint": "eslint .",
    "pretest": "npm run eslint",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 \"nyc --all node ./node_modules/.bin/ava --verbose\"",
    "test:watch": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "clean": "rimraf build",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --profile",
    "build:server": "cross-env NODE_ENV=production babel src/server --out-dir build/server --source-maps inline --copy-files",
    "build:shared": "cross-env NODE_ENV=production babel src/shared --out-dir build/shared --source-maps inline --copy-files",
    "prebuild": "npm run eslint",
    "build": "npm-run-all clean --parallel build:client build:server",
    "dev": "cross-env NODE_ENV=development \"nodemon --watch src/server src/server --exec 'npm run eslint && node -r babel-register'\"",
    "start": "cross-env NODE_ENV=production node build/server"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles",
      "./src/server/utils/setupTestEnv.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/client/reducers/index.js",
      "src/client/store/*.js",
      "src/client/routes.js",
      "src/server/utils/setupTestEnv.js",
      "src/server/utils/testHelpers.js",
      "node_modules",
      "build"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "React",
    "Webpack",
    "Redux",
    "Express",
    "Node"
  ],
  "author": "Sai Karthik Reddy Ginni <saikarthikreddyginni@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-root-import": "^4.1.8",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "imports-loader": "^0.7.1",
    "jsdom": "^9.12.0",
    "mock-css-modules": "^1.0.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.3.3",
    "promise-loader": "^1.0.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-test-renderer": "^15.5.4",
    "redux-ava": "^2.2.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "stats-webpack-plugin": "^0.6.0",
    "style-loader": "^0.16.1",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cross-env": "^4.0.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.6",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "fastclick": "^1.0.6",
    "hpp": "^0.2.2",
    "morgan": "^1.8.1",
    "normalize.css": "^6.0.0",
    "react": "^15.5.4",
    "react-css-modules": "^4.2.0",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
