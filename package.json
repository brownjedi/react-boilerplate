{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React Redux Template",
  "main": "build/index.js",
  "scripts": {
    "test": "npm run eslint && cross-env NODE_ENV=test node_modules/.bin/nyc --all node --harmony-proxies node_modules/.bin/ava --verbose",
    "test:watch": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "eslint": "eslint .",
    "clean": "rimraf build",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --profile",
    "build:server": "cross-env NODE_ENV=production babel src/server --out-dir build --source-maps inline --copy-files",
    "build": "npm-run-all clean --parallel build:client build:server",
    "dev": "cross-env NODE_ENV=development nodemon --watch src/server src/server --exec 'npm run eslint && node -r 'babel-register''",
    "start": "cross-env NODE_ENV=production node build/index.js"
  },
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.3"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles",
      "./src/server/utils/setupTestEnv.js"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/client/reducers/index.js",
      "src/client/store/*.js",
      "src/client/routes.js",
      "src/server/utils/setupTestEnv.js",
      "src/server/utils/testHelpers.js",
      "node_modules",
      "build"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "React",
    "Webpack",
    "Redux",
    "Express",
    "Node"
  ],
  "author": "Sai Karthik Reddy Ginni <saikarthikreddyginni@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.0.0",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "happypack": "^3.0.1",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "imports-loader": "^0.6.5",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mock-css-modules": "^1.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.2.0",
    "promise-loader": "^1.0.0",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-ava": "^2.2.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-immutable-state-invariant": "^1.2.3",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.6",
    "stats-webpack-plugin": "^0.4.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "babel-runtime": "^6.9.2",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "dotenv": "^2.0.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "fastclick": "^1.0.6",
    "hpp": "^0.2.1",
    "morgan": "^1.7.0",
    "normalize.css": "^5.0.0",
    "react": "^15.2.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.2.1",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^2.0.1"
  }
}
